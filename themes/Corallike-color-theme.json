{
  "name": "Corallike",
  "type": "dark",
  "colors": {
    "editor.background": "#151515",
    "editorGroupHeader.tabsBorder": "#333",
    "editorGroupHeader.tabsBackground": "#222",
    "tab.inactiveBackground": "#222",
    "tab.activeBackground": "#222",
    "tab.activeBorder": "#55f",
    "titleBar.inactiveBackground": "#222",
    "titleBar.activeBackground": "#222",
    "titleBar.border": "#333",
    "sideBar.background": "#222",
    "sideBar.border": "#333",
    "sideBarSectionHeader.background": "#222",
    "sideBarSectionHeader.border": "#333",
    "activityBar.background": "#222",
    "activityBar.border": "#333",
    "activityBar.activeBorder": "#55f",
    "activityBar.activeFocusBorder": "#55f",
    "activityBarBadge.background": "#55f",
    "statusBar.background": "#222",
    "statusBar.debuggingBackground": "#222",
    "statusBar.border": "#333",
    "scrollbar.shadow": "#151515",
    "menu.background": "#222",
    "menu.border": "#333",
    "menu.foreground": "#eee"
  },
  // "semanticTokenColors": { "property": "#" },
  "tokenColors": [
    // MY TOKEN COLORS
    { "scope": "entity.name.function", "settings": { "foreground": "#ffa" } },
    { "scope": "entity.name.method", "settings": { "foreground": "#ffa" } },
    { "scope": "entity.name.tag", "settings": { "foreground": "#fa5" } },
    { "scope": "entity.name.type", "settings": { "foreground": "#5af" } },
    { "scope": "entity.name.class", "settings": { "foreground": "#5af" } },

    { "scope": "function", "settings": { "foreground": "#ffa" } },
    { "scope": "constant", "settings": { "foreground": "#ffa" } },
    { "scope": "variable", "settings": { "foreground": "#5ff" } },
    { "scope": "keyword", "settings": { "foreground": "#5af" } },
    { "scope": "constant.language", "settings": { "foreground": "#5af" } },
    { "scope": "keyword.control", "settings": { "foreground": "#5fa" } },
    { "scope": "keyword.operator", "settings": { "foreground": "#eee" } },

    { "scope": "storage.type", "settings": { "foreground": "#5af" } },
    { "scope": "storage.modifier", "settings": { "foreground": "#5fa" } },

    { "scope": "string.regexp", "settings": { "foreground": "#5fa" } },
    { "scope": "invalid", "settings": { "foreground": "#f55" } },

    { "scope": "punctuation", "settings": { "foreground": "#eee" } },
    { "scope": "meta.brace.round", "settings": { "foreground": "#eee" } },
    { "scope": "meta.brace.square", "settings": { "foreground": "#eee" } },
    { "scope": "string", "settings": { "foreground": "#aaa" } },
    { "scope": "comment", "settings": { "foreground": "#555" } },

    {
      "name": "class, type and interface keywords",
      "scope": [
        "storage.type.class",
        "storage.type.type",
        "storage.type.interface"
      ],
      "settings": { "foreground": "#a5f" }
    },

    {
      "name": "HTML attributes",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#ffa" }
    },

    {
      "name": "HTML text",
      "scope": "text.html",
      "settings": { "foreground": "#eee" }
    },

    {
      "name": "style keys",
      "scope": "support.type",
      "settings": { "foreground": "#5af" }
    },

    {
      "name": "Style values",
      "scope": "meta.property-value",
      "settings": { "foreground": "#eee" }
    },

    {
      "name": "sass scripts",
      "scope": "support.function",
      "settings": { "foreground": "#ffa" }
    },

    {
      "name": "js object keys",
      "scope": "meta.object.member",
      "settings": { "foreground": "#5ff" }
    },

    {
      "name": "template literals",
      "scope": "meta.template",
      "settings": { "foreground": "#5af" }
    },

    // UNDEFINED TOKEN COLORS

    {
      "name": "super()",
      "scope": "variable.language",
      "settings": { "foreground": "#5af" }
    },

    { "scope": "meta.class", "settings": { "foreground": "#5af" } },

    {
      "name": "chained properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": { "foreground": "#5af" }
    },
    {
      "name": "this new void",
      "scope": [
        "variable.language.this",
        "keyword.operator.new",
        "keyword.operator.expression.void"
      ],
      "settings": { "foreground": "#a5f" }
    },

    {
      "name": "constructors",
      "scope": "support.class",
      "settings": { "foreground": "#5af" }
    },

