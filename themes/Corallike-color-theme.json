{
  "name": "Corallike",
  "type": "dark",
  "colors": {
    "editor.foreground": "#eee",
    "editor.background": "#111",
    "editorGroupHeader.tabsBorder": "#333",
    "editorGroupHeader.tabsBackground": "#222",
    "tab.inactiveBackground": "#222",
    "tab.activeBackground": "#222",
    "tab.activeBorder": "#55f",
    "titleBar.inactiveBackground": "#222",
    "titleBar.activeBackground": "#222",
    "titleBar.border": "#333",
    "sideBar.background": "#222",
    "sideBar.border": "#333",
    "sideBarSectionHeader.background": "#222",
    "sideBarSectionHeader.border": "#333",
    "activityBar.background": "#222",
    "activityBar.border": "#333",
    "activityBar.activeBorder": "#55f",
    "activityBar.activeFocusBorder": "#55f",
    "activityBarBadge.background": "#55f",
    "statusBar.background": "#222",
    "statusBar.debuggingBackground": "#222",
    "statusBar.border": "#333",
    "statusBarItem.remoteBackground": "#55f",
    "scrollbar.shadow": "#151515",
    "menu.background": "#222",
    "menu.border": "#333",
    "menu.foreground": "#eee"
  },
  "tokenColors": [
    // MY TOKEN COLORS

    { "scope": "entity.name.function", "settings": { "foreground": "#ffa" } },
    { "scope": "entity.name.method", "settings": { "foreground": "#ffa" } },
    { "scope": "entity.name.tag", "settings": { "foreground": "#f55" } },
    { "scope": "entity.name.type", "settings": { "foreground": "#5af" } },
    { "scope": "entity.name.class", "settings": { "foreground": "#5af" } },

    { "scope": "variable", "settings": { "foreground": "#5ff" } },

    { "scope": "constant.language", "settings": { "foreground": "#5af" } },
    { "scope": "constant.numeric", "settings": { "foreground": "#5ff" } },
    { "scope": "constant.numeric.css", "settings": { "foreground": "#ffa" } },
    { "scope": "constant.character", "settings": { "foreground": "#ffa" } },

    { "scope": "keyword.control", "settings": { "foreground": "#5fa" } },
    { "scope": "keyword.operator", "settings": { "foreground": "#eee" } },

    { "scope": "storage.type", "settings": { "foreground": "#5af" } },
    { "scope": "storage.modifier", "settings": { "foreground": "#a5f" } },

    { "scope": "invalid", "settings": { "foreground": "#f55" } },
    { "scope": "comment.todo", "settings": { "foreground": "#f55" } },

    { "scope": "string", "settings": { "foreground": "#aaa" } },
    { "scope": "string.regexp", "settings": { "foreground": "#5fa" } },

    {
      "name": "punctuation",
      "scope": ["punctuation", "meta.brace.round", "meta.brace.square"],
      "settings": { "foreground": "#eee" }
    },

    {
      "name": "${var}",
      "scope": "punctuation.definition.template-expression",
      "settings": { "foreground": "#5af" }
    },

    {
      "name": "comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#555" }
    },


    // HTML

    {
      "name": "HTML attributes",
      "scope": "entity.other.attribute-name.html",
      "settings": { "foreground": "#fa5" }
    },
    {
      "name": "HTML attribute values",
      "scope": "string.quoted.double.html",
      "settings": { "foreground": "#ffa" }
    },
    {
      "name": "HTML text",
      "scope": "text.html",
      "settings": { "foreground": "#eee" }
    },

    // sass & css

    {
      "name": "classes",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#ffa" }
    },

    {
      "name": "style keys",
      "scope": "support.type",
      "settings": { "foreground": "#5af" }
    },

    {
      "name": "Style values",
      "scope": "meta.property-value",
      "settings": { "foreground": "#eee" }
    },

    { "scope": "sass.use", "settings": { "foreground": "#5fa" } },

    {
      "name": "sass scripts",
      "scope": "support.function",
      "settings": { "foreground": "#ffa" }
    },

    // TYPESCRIPT

    {
      "name": "class, type and interface keywords",
      "scope": [
        "storage.type.class",
        "storage.type.type",
        "storage.type.interface",
        "storage.type.namespace"
      ],
      "settings": { "foreground": "#a5f" }
    },

    {
      "name": "object keys",
      "scope": "meta.object.member",
      "settings": { "foreground": "#5ff" }
    },

    {
      "scope": ["meta.interface", "meta.class"],
      "settings": { "foreground": "#5af" }
    },

    {
      "name": "this new void",
      "scope": [
        "variable.language.this",
        "keyword.operator.new",
        "keyword.operator.expression.void"
      ],
      "settings": { "foreground": "#a5f" }
    },

    {
      "name": "constructors",
      "scope": "support.class",
      "settings": { "foreground": "#5af" }
    },

    // MARKDOWN

    { "scope": "markup.bold", "settings": { "fontStyle": "bold" } },
    { "scope": "markup.italic", "settings": { "fontStyle": "italic" } },
    {
      "name": "MD Titles",
      "scope": "markup.heading",
      "settings": { "foreground": "#5af", "fontStyle": "bold" }
    },
    {
      "name": "List punctuation",
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": { "foreground": "#5af" }
    },
    {
      "name": "MD Inline",
      "scope": "markup.inline.raw",
      "settings": { "foreground": "#5ff", "fontStyle": "italic" }
    },

    // UNDEFINED TOKEN COLORS

    {
      "name": "super()",
      "scope": "variable.language",
      "settings": { "foreground": "#5af" }
    },

    {
      "name": "chained properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": { "foreground": "#5af" }
    }
  ]
}
